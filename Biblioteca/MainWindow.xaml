<Window x:Class="Biblioteca.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Biblioteca"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="txtEmail" HorizontalAlignment="Center" Margin="0,227,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="174" Height="20" />
        <TextBox x:Name="txtCpf" HorizontalAlignment="Center" Margin="0,199,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="174" />
        <Label Content="Cadastro&#xD;&#xA;" HorizontalAlignment="Center" Margin="0,152,0,0" VerticalAlignment="Top" Width="288" RenderTransformOrigin="0.51,0.517" />
        <Button Content="Cadastrar" HorizontalAlignment="Center" Margin="0,333,0,0" VerticalAlignment="Top" Width="112" Click="Button_Click"/>
        <TextBox x:Name="txtFullName" HorizontalAlignment="Center" Margin="0,260,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="174" RenderTransformOrigin="-0.202,0.73"/>
        <TextBox x:Name="txtDateBirth" HorizontalAlignment="Center" Margin="0,292,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="174" />

    </Grid>
</Window>
